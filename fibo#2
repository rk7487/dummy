#include<iostream>
using namespace std;
int fib(int n)
{
	if (n <= 1)
		return n;

	return fib(n - 1)%10 + fib(n - 2)%10;
}


int main()
{
	 
long t;
cin>>t;
while(t>0)
{
	
    unsigned long int n;
	int m=1;
	cin>>n;
	
	while(n>1)
	{
	    n=n/2;
	    m=2*m;
            m=m%60;
	}
	m=m-1;
	cout<<fib(m)<<endl;
	
	
	
	t--;
}
return 0;
}
