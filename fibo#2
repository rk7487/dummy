#include<iostream>
#include<math.h>
using namespace std;
int fib(int n)
{
	if (n <= 1)
		return n;

	return fib(n - 1) + fib(n - 2);
}


int main()
{
	 
long t;
cin>>t;
while(t>0)
{
	
     long a=0,b=1,num,n;
	int m=1,j=0;
	cin>>n;
	
	while(n>0)
	{
	    n=n/2;
	    j++;
	    m=2*m;
	}
	m=m-1;
	m=m/2;
	cout<<fib(m)%10<<endl;
	
	
	
	t--;
}
return 0;
}
