#include<stdio.h>
int fib(int n)
{
	if (n <= 1)
		return n;

	return fib(n - 1) + fib(n - 2);
}


int main()
{
	 
long t;
scanf("ld",&t);
while(t>0)
{
	
     long a=0,b=1,num,n;
	int m=1,j=0;
	scanf("%ld",&n);
	
	while(n)
	{
	    n=n/2;
	    j++;
	    m=2*m;
	}
	m=m-1;
	m=m/2;
	j=fib(m)%10;
	printf("\n%d",j);
	t=t-1;
}
return 0;
}
