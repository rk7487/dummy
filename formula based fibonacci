#include<iostream>
#include<math.h>
using namespace std;


int main()
{
	 
int t;
unsigned long int n, m,j,a;
cin>>t;
while(t>0)
{
	
      m=1,j=0;
	cin>>n;
	if(n>0)
	{
	while(n>0)
	{
	    n=n/2;
	    m=2*m;
	}
	m=m-1;
	m=m/2;
	a=((pow(1.618,m)-pow(-0.618,m))/2.236)+1;
	cout<<a%10;
	}
	t--;
	cout<<endl;
}
return 0;
}
